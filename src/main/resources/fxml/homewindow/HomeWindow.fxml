<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<StackPane fx:id="homeWindowSP"
	stylesheets="@../../css/Adreana.css"
	xmlns="http://javafx.com/javafx/8.0.141"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.rohitawate.everest.controllers.HomeWindowController">
	<children>
		<SplitPane fx:id="splitPane" dividerPositions="0.3">
			<items>
				<TabPane fx:id="toolsTabPane" tabMaxHeight="50.0"
					tabMaxWidth="100.0" tabMinHeight="50.0" tabMinWidth="50.0"
					SplitPane.resizableWithParent="false" maxWidth="450.0"
					minWidth="400.0">
					<Tab fx:id="searchTab" text="Search" closable="false">
						<fx:include fx:id="searchPane" source="SearchPane.fxml" />
					</Tab>
					<Tab fx:id="collectionsTab" text="Collections" closable="false">
						<fx:include fx:id="collectionsPane"
							source="CollectionsPane.fxml" />
					</Tab>
				</TabPane>
				<TabPane fx:id="homeWindowTabPane"
					tabClosingPolicy="ALL_TABS" tabMaxHeight="30.0" tabMaxWidth="200.0"
					tabMinHeight="30.0" tabMinWidth="200.0"
					SplitPane.resizableWithParent="false" />
			</items>
		</SplitPane>
	</children>
</StackPane>
